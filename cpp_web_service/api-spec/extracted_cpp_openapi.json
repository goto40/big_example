            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MyDto"
                }
              }
            }
          },
          "tags": [
            "demo"
          ],
          "responses": {
            "200": {
              "description": "OK"
            }
          },
          "parameters": []
        }
      }
    },
    "components": {
      "schemas": {
        "MyDto": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "description": "this is our main demo data!"
            }
          },
          "required": [
            "message"
          ]
        }
      }
    }
